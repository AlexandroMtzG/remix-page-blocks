{
  "name": "remix-page-blocks",
  "version": "0.7.0",
  "description": "A collection of functional blocks: Header, Footer, Hero, Banner, Features, Logo Clouds, Community, Embeded Video, Testimonials, FAQ, Contact, Newsletter, and more.",
  "homepage": "https://remix-page-blocks.vercel.app",
  "repository": "https://github.com/AlexandroMtzG/remix-page-blocks",
  "bugs": "https://github.com/AlexandroMtzG/remix-page-blocks/issues",
  "author": "Alexandro Martinez",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && cross-env NODE_ENV=production remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"cross-env NODE_ENV=development remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "lint": "eslint --ext .tsx,.ts .",
    "postinstall": "remix setup node",
    "typecheck": "tsc -b",
    "prettier": "prettier --write '**/*.{ts,tsx}'"
  },
